<?xml version="1.0" encoding="utf-8"?>
<?xaml-comp compile="true" ?>

<ResourceDictionary xmlns="http://xamarin.com/schemas/2014/forms"
                    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml">

    <x:Double x:Key="SpacingSmall">5</x:Double>
    <x:Double x:Key="SpacingMedium">10</x:Double>
    <x:Double x:Key="SpacingLarge">15</x:Double>

    <OnPlatform x:TypeArguments="GridLength"
                x:Key="GridLengthNavigationBarHeight">
        <On Platform="iOS" Value="44" />
        <On Platform="Android, UWP" Value="56" />
    </OnPlatform>

    <x:Double x:Key="NavigationBar">15</x:Double>

    <Color x:Key="TextPrimaryColor">#B000000</Color>
    <Color x:Key="TextSecondaryColor">#8000000</Color>
    <Color x:Key="TextTernaryColor">#C8C8C8</Color>

</ResourceDictionary>